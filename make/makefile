vpath % $(jgit)/jsrc

%.o: %.c
	$(CC) -c $(CFLAGS) $(CFLAGS_SIMD) $(CFLAGS_FMA) $(CFLAGS_AESNI) -o$@ $^

CFLAGS=$(COMPILE)
$(OBJS_FMA) : CFLAGS_FMA := -mfma 
$(OBJS_AESNI) : CFLAGS_AESNI := -maes
$(TARGET) : $(OBJS) $(OBJS_FMA) $(OBJS_AESNI) $(OBJS_AESARM)
	$(CC)  $(OBJS) $(OBJS_FMA) $(OBJS_AESNI) $(OBJS_AESARM) $(OBJS_SHAASM) $(LINK)
